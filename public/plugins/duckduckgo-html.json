{
  "openapi": "3.1.0",
  "info": {
    "title": "duckduckgo html",
    "description": "a search engine. useful for current events. input should be a search query.",
    "version": "v1.0.0"
  },
  "servers": [
    {
      "url": "https://html.duckduckgo.com"
    }
  ],
  "paths": {
    "/html": {
      "get": {
        "operationId": "DuckDuckGoLiteSearch",
        "description": "a search engine. useful for current events. input should be a search query.",
        "deprecated": false,
        "parameters": [
          {
            "name": "q",
            "in": "query",
            "required": true,
            "description": "keywords for query.",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "kl",
            "in": "query",
            "description": "region or language, e.g. wt-wt, us-en, uk-en.",
            "schema": {
              "type": "string"
            }
          }
        ]
      }
    }
  },
  "components": {
    "schemas": {}
  }
}